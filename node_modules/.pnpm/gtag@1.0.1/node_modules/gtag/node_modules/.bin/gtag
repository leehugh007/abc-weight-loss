#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/abc-weight-loss-showcase/node_modules/.pnpm/gtag@1.0.1/node_modules/gtag/node_modules:/home/ubuntu/abc-weight-loss-showcase/node_modules/.pnpm/gtag@1.0.1/node_modules:/home/ubuntu/abc-weight-loss-showcase/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/abc-weight-loss-showcase/node_modules/.pnpm/gtag@1.0.1/node_modules/gtag/node_modules:/home/ubuntu/abc-weight-loss-showcase/node_modules/.pnpm/gtag@1.0.1/node_modules:/home/ubuntu/abc-weight-loss-showcase/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/sh" ]; then
  exec "$basedir/sh"  "$basedir/../../gtag.sh" "$@"
else
  exec sh  "$basedir/../../gtag.sh" "$@"
fi
